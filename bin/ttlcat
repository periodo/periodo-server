#! /usr/bin/env python3

import fileinput

base = None
prefixes = []
lines = []

for line in fileinput.input():
    if line.startswith('@base'):
        if base is None:
            base = line
        elif base != line:
            raise Exception('Already have %s' % base)
    elif line.startswith('@prefix'):
        prefixes.append(line)
    else:
        lines.append(line)

print(base, end='')

for prefix in sorted(set(prefixes)):
    print(prefix, end='')

last_was_blank = False
for line in lines:
    # collapse multiple blank lines
    if line.strip() == '':
        if last_was_blank:
            continue
        else:
            last_was_blank = True
    else:
        last_was_blank = False
    print(line, end='')
